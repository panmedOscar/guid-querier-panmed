<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.lang.String?>
<?import javafx.collections.*?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane id="gridPane" fx:id="mainPane" alignment="CENTER"
	maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	prefHeight="670.0" prefWidth="870.0" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/2.2" fx:controller="tw.edu.ym.guid.querier.FXMLController">
	<children>
		<MenuBar GridPane.columnIndex="0" GridPane.rowIndex="0">
			<menus>
				<Menu mnemonicParsing="false" text="匯入試算表">
					<items>
						<MenuItem fx:id="importMI" mnemonicParsing="false" text="選擇資料夾..." />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="安全性">
					<items>
						<MenuItem fx:id="securityMI" mnemonicParsing="false"
							text="修改密碼" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="備份">
					<items>
						<MenuItem fx:id="backupMI" mnemonicParsing="false" text="選擇備份路徑..." />
					</items>
				</Menu>
			</menus>
		</MenuBar>
		<GridPane GridPane.columnIndex="0" GridPane.rowIndex="1">
			<children>
				<TextField fx:id="searchTF" maxWidth="1.7976931348623157E308"
					GridPane.columnIndex="0" GridPane.rowIndex="0" />
				<Button fx:id="searchBtn" maxWidth="100.0" mnemonicParsing="false"
					prefWidth="100.0" text="搜尋" GridPane.columnIndex="1"
					GridPane.rowIndex="0" />
			</children>
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" />
				<ColumnConstraints hgrow="NEVER" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints vgrow="NEVER" />
			</rowConstraints>
		</GridPane>
		<TableView fx:id="piiTable" maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" GridPane.columnIndex="0"
			GridPane.rowIndex="2">
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
			<columns>
				<TableColumn text="編碼日期">
					<cellValueFactory>
						<PropertyValueFactory property="編碼日期" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="Guid">
					<cellValueFactory>
						<PropertyValueFactory property="Guid" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="mrnCol" text="Mrn">
					<cellValueFactory>
						<PropertyValueFactory property="Mrn" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="身份證字號">
					<cellValueFactory>
						<PropertyValueFactory property="身份證字號" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="姓氏">
					<cellValueFactory>
						<PropertyValueFactory property="姓氏" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="名字">
					<cellValueFactory>
						<PropertyValueFactory property="名字" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="生日">
					<columns>
						<TableColumn text="月">
							<cellValueFactory>
								<PropertyValueFactory property="出生月" />
							</cellValueFactory>
						</TableColumn>
						<TableColumn text="日">
							<cellValueFactory>
								<PropertyValueFactory property="出生日" />
							</cellValueFactory>
						</TableColumn>
						<TableColumn text="年">
							<cellValueFactory>
								<PropertyValueFactory property="出生年" />
							</cellValueFactory>
						</TableColumn>
					</columns>
				</TableColumn>
				<TableColumn text="性別">
					<cellValueFactory>
						<PropertyValueFactory property="性別" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="telCol" text="聯絡電話">
					<cellValueFactory>
						<PropertyValueFactory property="聯絡電話" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="addrCol" text="地址">
					<cellValueFactory>
						<PropertyValueFactory property="地址" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="drCol" text="收案醫師">
					<cellValueFactory>
						<PropertyValueFactory property="收案醫師" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="hospitalCol" text="收案醫院名稱">
					<cellValueFactory>
						<PropertyValueFactory property="收案醫院名稱" />
					</cellValueFactory>
				</TableColumn>
			</columns>
		</TableView>
	</children>
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" />
	</columnConstraints>
	<padding>
		<Insets bottom="4.0" left="4.0" right="4.0" />
	</padding>
	<rowConstraints>
		<RowConstraints vgrow="NEVER" />
		<RowConstraints vgrow="NEVER" />
		<RowConstraints vgrow="SOMETIMES" />
	</rowConstraints>
</GridPane>
