<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.lang.String?>
<?import javafx.collections.*?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane id="gridPane" fx:id="mainPane" alignment="CENTER"
	maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	prefWidth="670.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
	fx:controller="tw.edu.ym.guid.querier.FXMLController">
	<children>
		<GridPane hgap="5.0" GridPane.columnIndex="0"
			GridPane.rowIndex="0">
			<children>
				<Label text="使用者帳號" prefWidth="80.0" maxWidth="80.0"
					GridPane.columnIndex="0" GridPane.rowIndex="0" />
				<TextField fx:id="username" maxWidth="1.7976931348623157E308"
					GridPane.columnIndex="1" GridPane.rowIndex="0" />
				<Label text="登入密碼" prefWidth="80.0" maxWidth="80.0"
					GridPane.columnIndex="2" GridPane.rowIndex="0" />
				<PasswordField fx:id="password" maxWidth="1.7976931348623157E308"
					GridPane.columnIndex="3" GridPane.rowIndex="0" />
				<Button fx:id="loginBtn" text="登入" mnemonicParsing="false"
					prefWidth="100.0" maxWidth="100.0" GridPane.columnIndex="4"
					GridPane.rowIndex="0" />
			</children>
			<columnConstraints>
				<ColumnConstraints hgrow="NEVER" />
				<ColumnConstraints hgrow="SOMETIMES" />
				<ColumnConstraints hgrow="NEVER" />
				<ColumnConstraints hgrow="SOMETIMES" />
				<ColumnConstraints hgrow="NEVER" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints vgrow="NEVER" />
			</rowConstraints>
		</GridPane>
		<GridPane hgap="5.0" GridPane.columnIndex="0"
			GridPane.rowIndex="1">
			<children>
				<TextField fx:id="searchField" maxWidth="1.7976931348623157E308"
					GridPane.columnIndex="0" GridPane.rowIndex="0" />
				<Button fx:id="searchBtn" mnemonicParsing="false" prefWidth="100.0"
					maxWidth="100.0" text="搜尋" GridPane.columnIndex="1"
					GridPane.rowIndex="0" />
			</children>
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" />
				<ColumnConstraints hgrow="NEVER" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints vgrow="NEVER" />
			</rowConstraints>
		</GridPane>
		<TableView fx:id="guidList" maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" GridPane.columnIndex="0"
			GridPane.rowIndex="2">
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
			<columns>
				<TableColumn text="編碼日期">
					<cellValueFactory>
						<PropertyValueFactory property="編碼日期" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="GUID">
					<cellValueFactory>
						<PropertyValueFactory property="GUID" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="MRN">
					<cellValueFactory>
						<PropertyValueFactory property="MRN" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="身份證字號">
					<cellValueFactory>
						<PropertyValueFactory property="身份證字號" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="姓氏">
					<cellValueFactory>
						<PropertyValueFactory property="姓氏" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="名字">
					<cellValueFactory>
						<PropertyValueFactory property="名字" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="出生月">
					<cellValueFactory>
						<PropertyValueFactory property="出生月" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="出生日">
					<cellValueFactory>
						<PropertyValueFactory property="出生日" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="出生年">
					<cellValueFactory>
						<PropertyValueFactory property="出生年" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="性別">
					<cellValueFactory>
						<PropertyValueFactory property="性別" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="聯絡電話">
					<cellValueFactory>
						<PropertyValueFactory property="聯絡電話" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="地址">
					<cellValueFactory>
						<PropertyValueFactory property="地址" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="收案醫師">
					<cellValueFactory>
						<PropertyValueFactory property="收案醫師" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="收案醫院名稱">
					<cellValueFactory>
						<PropertyValueFactory property="收案醫院名稱" />
					</cellValueFactory>
				</TableColumn>
			</columns>
		</TableView>
	</children>
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" />
	</columnConstraints>
	<padding>
		<Insets bottom="4.0" left="4.0" right="4.0" top="2.0" />
	</padding>
	<rowConstraints>
		<RowConstraints vgrow="NEVER" />
		<RowConstraints vgrow="NEVER" />
		<RowConstraints vgrow="SOMETIMES" />
	</rowConstraints>
</GridPane>
